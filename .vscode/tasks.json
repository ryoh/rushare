{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "type": "cargo",
      "command": "build",
      "group": "build",
      "problemMatcher": [
        "$rustc"
      ]
    },
    {
      "type": "cargo",
      "command": "run",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher":"$rustc"
    }
  ]
}